  "text.autoconfig.onebar.title": "OneBar configuration",
  "text.autoconfig.onebar.category.default": "General",
  "text.autoconfig.onebar.option.showOneBar": "Enable OneBar",
  "text.autoconfig.onebar.option.showOneBar.@Tooltip": "Disabling will show vanilla HUD instead",
  "text.autoconfig.onebar.option.healthEstimates": "Effect estimates",
  "text.autoconfig.onebar.option.healthEstimates.@Tooltip": "Toggles the texts and bars for (natural) regeneration, hunger effect, poison etc.",
  "text.autoconfig.onebar.option.uhcMode": "UHC mode",
  "text.autoconfig.onebar.option.uhcMode.@Tooltip": "Disables natural regen estimates, if effect estimates are enabled",
  "text.autoconfig.onebar.option.disableHunger": "Disable hunger visibility",
  "text.autoconfig.onebar.option.disableHunger.@Tooltip": "Hides the hunger bar/text, hunger effect and\nstarvation estimates etc, meant for certain minigames",
  "text.autoconfig.onebar.option.backgroundColor.@PrefixText": "§8Every color is preceded by the opacity value in hex, see mod page for help",
  "text.autoconfig.onebar.option.backgroundColor": "Background color",
  "text.autoconfig.onebar.option.enableGradient": "Enable gradient",
  "text.autoconfig.onebar.option.enableGradient.@Tooltip": "Makes all bars' colors gradually shift from base color to darker or lighter",
  "text.autoconfig.onebar.option.gradientShift": "Gradient shift",
  "text.autoconfig.onebar.option.gradientShift.@Tooltip": "Percentage of color change\n0 - darkest\n100 - no gradient\n200 - lightest\nThe gradient is harder to notice with dark background colors, such as black.",
  "text.autoconfig.onebar.category.textSettings": "Text",
  "text.autoconfig.onebar.option.textSettings.showText": "Show numbers and symbols on OneBar",
  "text.autoconfig.onebar.option.textSettings.showText.@Tooltip": "Disabling will still show numeric absorption health and hardcore indicator (if exist)",
  "text.autoconfig.onebar.option.textSettings.textColor": "Text color",
  "text.autoconfig.onebar.option.textSettings.estimatesItalic": "Effect estimates in §oitalic§r",
  "text.autoconfig.onebar.option.textSettings.estimatesItalic.@Tooltip": "If effect estimates are enabled",
  "text.autoconfig.onebar.option.textSettings.estimatesParentheses": "Parentheses around effect estimates",
  "text.autoconfig.onebar.option.textSettings.estimatesParentheses.@Tooltip": "E.g. health-related effect estimates will be grouped; if effect estimates are enabled",
  "text.autoconfig.onebar.option.textSettings.use.emoji": "Use emoji as symbols",
  "text.autoconfig.onebar.option.textSettings.useEmoji.@Tooltip": "For example, when you are burning, you'll see:\n\uD83D\uDD251× with this option enabled\nbu1× with this option disabled\nDisabling is recommended when your font doesn't support the emoji.",
  "text.autoconfig.onebar.option.textSettings.extraSymbols": "Additional symbols",
  "text.autoconfig.onebar.option.textSettings.extraSymbols.@Tooltip": "Shows emoji or abbreviations for health, hunger and absorption as well",
  "text.autoconfig.onebar.option.textSettings.useFractions": "Vanilla-like fractions",
  "text.autoconfig.onebar.option.textSettings.useFractions.@Tooltip": "E.g. show 9,5 instead of 19 health",
  "text.autoconfig.onebar.option.textSettings.useFractionsPadZeroes": "Vanilla-like fractions: pad zeroes",
  "text.autoconfig.onebar.option.textSettings.useFractionsPadZeroes.@Tooltip": "If vanilla-like fractions are enabled: adds zeroes to even numbers.\nThis is beneficial if the numbers change a lot as it will reduce flickering.",
  "text.autoconfig.onebar.option.textSettings.rawHealth": "Raw health value",
  "text.autoconfig.onebar.option.textSettings.rawHealth.@Tooltip": "Shows the decimal-based health value the game uses internally\nand OneBar uses for the bar, rounded to two decimals.\nOnly for your or mount's main health, not estimates etc.\nIgnores the vanilla-like fractions option.",
  "text.autoconfig.onebar.category.goodThings": "Good effects",
  "text.autoconfig.onebar.option.goodThings.healthColor": "Health color",
  "text.autoconfig.onebar.option.goodThings.naturalRegenerationColor": "Natural regeneration color",
  "text.autoconfig.onebar.option.goodThings.regenerationColor": "Regeneration effect color",
  "text.autoconfig.onebar.option.goodThings.showResistance": "Resistance percent",
  "text.autoconfig.onebar.option.goodThings.showResistance.@Tooltip": "Show resistance effect level as percentage, e.g. level 2 shows 40% (level × 20%)",
  "text.autoconfig.onebar.option.goodThings.showInvisibility": "Invisibility indicator",
  "text.autoconfig.onebar.option.goodThings.showInvisibility.@Tooltip": "Shows an indicator when you have invisibility effect;\ncrosses it out when wearing armor/offhand item,\nhave glowing effect or arrow(s) stuck in you.",
  "text.autoconfig.onebar.option.goodThings.showTotemOfUndying": "Totem of Undying indicator",
  "text.autoconfig.onebar.option.goodThings.showTotemOfUndying.@Tooltip": "Shows the amount of Totem of Undyings in your inventory;\ncrossed out when not held in main/offhand.",
  "text.autoconfig.onebar.option.goodThings.heldFoodHungerBar": "Food level restoration by held food",
  "text.autoconfig.onebar.option.goodThings.heldFoodHungerBar.@Tooltip": "Shows an overlay on hunger bar when holding any food item and player has hunger",
  "text.autoconfig.onebar.option.goodThings.heldFoodHungerWasteColor": "Food level restoration color: wasted",
  "text.autoconfig.onebar.option.goodThings.heldFoodHungerGoodColor": "Food level restoration color: exact or less",
  "text.autoconfig.onebar.category.badThings": "Bad effects",
  "text.autoconfig.onebar.option.badThings.hungerColor": "Hunger color",
  "text.autoconfig.onebar.option.badThings.hungerEffectColor": "Hunger effect color",
  "text.autoconfig.onebar.option.badThings.showHungerDecreasing": "Getting hungrier indicator",
  "text.autoconfig.onebar.option.badThings.showHungerDecreasing.@Tooltip": "Meaning zero saturation - various activities will increase the hunger",
  "text.autoconfig.onebar.option.badThings.airColor": "Air color (underwater)",
  "text.autoconfig.onebar.option.badThings.freezeColor": "Freezing color",
  "text.autoconfig.onebar.option.badThings.showFire": "Burning bar and damage multiplier",
  "text.autoconfig.onebar.option.badThings.showFire.@Tooltip": "1× - burning\n2× - in fire\n3× - in soul fire\n4× - in lava",
  "text.autoconfig.onebar.option.badThings.fireColor": "Burning color",
  "text.autoconfig.onebar.option.badThings.poisonColor": "Poison color",
  "text.autoconfig.onebar.option.badThings.witherColor": "Wither color",
  "text.autoconfig.onebar.option.badThings.showLevitation": "Levitation bar",
  "text.autoconfig.onebar.option.badThings.showLevitation.@Tooltip": "Shows the levitation effect time as a bar and indicator,\ncrossed out while in water",
  "text.autoconfig.onebar.option.badThings.levitationColor": "Levitation color",
  "text.autoconfig.onebar.option.badThings.showFallHeight": "Fall height indicator (experimental)",
  "text.autoconfig.onebar.option.badThings.showFallHeight.@Tooltip": "Tells you the amount of blocks you're about to fall in-air,\nwhile having levitation effect or while crouching on an edge.\nCurrently does not consider target block type, your effects and enchants.",
  "text.autoconfig.onebar.option.badThings.showGlowing": "Glowing indicator",
  "text.autoconfig.onebar.option.badThings.showGlowing.@Tooltip": "Shows an indicator when you have glowing effect from a spectral arrow",
  "text.autoconfig.onebar.option.badThings.showOmens": "Omen levels",
  "text.autoconfig.onebar.option.badThings.showOmens.@Tooltip": "Indicates Bad Omen, Raid Omen and Trial Omen effects.\nBad Omen does not have effects on its own, but may transform into other omens, keeping the level.\nRaid Omen levels over one indicate extra waves and chances of enchanted weapons in raids.\nTrial Omen levels are multipliers of duration, e.g. level 2 means 2×15=30 minutes of ominous trial.",
  "text.autoconfig.onebar.option.badThings.showWarden": "Warden anger level",
  "text.autoconfig.onebar.option.badThings.showWarden.@Tooltip": "The closest warden's anger level towards the player or other causes.\n20-11 - angry, chasing\n10-6 - agitated\n5-0 - calm, can despawn",
  "text.autoconfig.onebar.option.badThings.wardenColor": "Warden anger color",
  "text.autoconfig.onebar.option.badThings.showInfested": "Infested indicator",
  "text.autoconfig.onebar.option.badThings.showInfested.@Tooltip": "Shows an indicator when you have the Infested effect,\nwhich may summon silverfish upon getting hit",
  "text.autoconfig.onebar.option.badThings.showPostDeathEffects": "Post-death effect indicator",
  "text.autoconfig.onebar.option.badThings.showPostDeathEffects.@Tooltip": "Shows an indicator when you have Weaving (cobwebs),\nOozing (slimes) or Wind Charged effects,\nwhich may summon the respective thing upon death",
  "text.autoconfig.onebar.category.entity": "Rideable entities",
  "text.autoconfig.onebar.option.entity.healthColor": "Health color",
  "text.autoconfig.onebar.option.entity.showMountJump": "Mount jump bar",
  "text.autoconfig.onebar.option.entity.showMountJump.@Tooltip": "Horse: vertical bar below crosshair\ncamel: horizontal bar below crosshair",
  "text.autoconfig.onebar.option.entity.showMountJumpText": "Horse jump height text",
  "text.autoconfig.onebar.option.entity.showMountJumpText.@Tooltip": "Shows approximate horse jump height in blocks,\ncamels are not yet supported",
  "text.autoconfig.onebar.option.entity.jumpColor": "Jump color",
  "text.autoconfig.onebar.option.entity.showMountCooldown": "Camel cooldown bar",
  "text.autoconfig.onebar.option.entity.showMountCooldown.@Tooltip": "Shows a decreasing cooldown bar that indicates when\nthe camel can jump or move again",
  "text.autoconfig.onebar.option.entity.showMountCooldownText": "Camel cooldown timer text",
  "text.autoconfig.onebar.option.entity.showMountCooldownText.@Tooltip": "Negative seconds before\nthe camel can jump or move again",
  "text.autoconfig.onebar.option.entity.cooldownColor": "Cooldown color",
  "text.autoconfig.onebar.category.armor": "Armor",
  "text.autoconfig.onebar.option.armor.showArmorBar": "Armor bar",
  "text.autoconfig.onebar.option.armor.showArmorBar.@Tooltip": "Thin bar above OneBar; also for ridden horses",
  "text.autoconfig.onebar.option.armor.armorColor": "Armor color",
  "text.autoconfig.onebar.option.armor.showArmorDurabilityBar": "Armor durability bar",
  "text.autoconfig.onebar.option.armor.showArmorDurabilityBar.@Tooltip": "Overlay on armor bar, width adapts to armor bar or armor bar segment",
  "text.autoconfig.onebar.option.armor.armorDurabilityColor": "Armor durability color",
  "text.autoconfig.onebar.option.armor.showSegmentedArmorBar": "Segmented armor bar",
  "text.autoconfig.onebar.option.armor.showSegmentedArmorBar.@Tooltip": "Shows armor bar in four separate segments, to allow seeing\nthe armor/durability value for each individual piece.\nNote: customized armor values (e.g. a chestplate that gives 20 armor)\nwill not be reflected - vanilla bug.",
  "text.autoconfig.onebar.option.armor.showElytraDurabilityBar": "Elytra durability bar",
  "text.autoconfig.onebar.option.armor.showElytraDurabilityBar.@Tooltip": "Normal armor bar: displayed as a full-width overlay only when flying;\nsegmented armor bar: displayed constantly as a diamond-width bar at chest segment.",
  "text.autoconfig.onebar.option.armor.elytraDurabilityColor": "Elytra durability color",
  "text.autoconfig.onebar.option.armor.showMobHeads": "Mob head indicator",
  "text.autoconfig.onebar.option.armor.showMobHeads.@Tooltip": "Shows an indicator when wearing a skeleton, creeper or\nzombie mob head; carved pumpkin; any golden armor item.\nWorn mob heads reduce respective mob detection range by 50%;\ncarved pumpkin will prevent aggrevating endermen by looking;\nany golden armor item will prevent piglin aggrevation.",
  "text.autoconfig.onebar.category.otherBars": "Other bars",
  "text.autoconfig.onebar.option.otherBars.adaptiveXpBar": "Adaptive experience indicator",
  "text.autoconfig.onebar.option.otherBars.adaptiveXpBar.@Tooltip": "Hides the experience level and/or bar when the value is zero",
  "text.autoconfig.onebar.option.otherBars.xpColor": "Experience level/bar color",
  "text.autoconfig.onebar.option.otherBars.lapisCounter": "Lapis Lazuli counter",
  "text.autoconfig.onebar.option.otherBars.lapisCounter.@Tooltip": "A counter next to XP level that counts Lapis Lazuli items/blocks in your inventory",
  "text.autoconfig.onebar.option.otherBars.lapisTimesEnchantable": "Lapis Lazuli counter: show times enchantable maxed out",
  "text.autoconfig.onebar.option.otherBars.lapisTimesEnchantable.@Tooltip": "Instead of raw value, shows how many times can you do max enchants with XP level 30+",
  "text.autoconfig.onebar.option.otherBars.lapisColor": "Lapis Lazuli counter color",
  "text.autoconfig.onebar.option.otherBars.showSaturationBar": "Saturation bar",
  "text.autoconfig.onebar.option.otherBars.showSaturationBar.@Tooltip": "Thin bar below OneBar",
  "text.autoconfig.onebar.option.otherBars.saturationColor": "Saturation color",
  "text.autoconfig.onebar.option.otherBars.hotbarTooltipsDown": "Hotbar item tooltip down",
  "text.autoconfig.onebar.option.otherBars.hotbarTooltipsDown.@Tooltip": "Dynamically moves the hotbar selected item tooltip further down",
  "text.autoconfig.onebar.option.otherBars.compatibilityMode": "Compatibility mode",
  "text.autoconfig.onebar.option.otherBars.compatibilityMode.@Tooltip": "Allows some other mods to show their HUD elements around OneBar"
}
